package com.bridgelabz.designpattern;

public class EagerInitialization {
	
	static EagerInitialization eagerInitialization;

	private EagerInitialization() {
		
		System.out.println("Instance created");
	}
	
	public static EagerInitialization getInstance() {
		
		if(eagerInitialization == null){
		
			eagerInitialization = new EagerInitialization();  //object is created
		}
		
		return eagerInitialization;
		
	}
	
	public static void main(String[] args) {
		
		EagerInitialization obj = getInstance();
		EagerInitialization obj1 = getInstance();
		
	}
}
