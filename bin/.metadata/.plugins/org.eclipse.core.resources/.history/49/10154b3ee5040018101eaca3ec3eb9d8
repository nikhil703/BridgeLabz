package com.bridgelabz.algorithmprograms;

import com.bridgelabz.utility.Utility;

public class UtilityCode {

	public static void main(String[] args) {
		
		int array[] = new int[3]; 
		Utility utility = new Utility();
//		utility.start();
//		
//		System.out.println("********* Binary Search for String *********");
//		System.out.println();
//		utility.binarySearchForStr();
//		array[0]=utility.stop();
//		
//		utility.start();
		System.out.println("********* Insertion Sort for Integer *********");
		utility.insertionSort();
		array[1] = utility.stop();
		
		utility.start();
		System.out.println("********* Insertion Sort for String *********");
		utility.insertionSortForString();
		array[2] = utility.stop();
		
		utility.start();
		System.out.println("********* Bubble Sort for String *********");
		utility.bubbleSorForString();
		array[3] = utility.stop();
		
		utility.start();
		System.out.println("********* Bubble Sort for Integer *********");
		utility.bubbleSortForInt();
		array[4] = utility.stop();
		
		for(int i : array){
			
			System.out.println("The array is: "+i);
		}
	}
}
