<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>FirstHttpServlet</servlet-name>
		<servlet-class>com.briglabz.httpservlet.FirstHttpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FirstHttpServlet</servlet-name>
		<url-pattern>/FirstHttpServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>inget</servlet-name>
		<servlet-class>com.briglabz.httpservlet.DogetExample</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>inget</servlet-name>
		<url-pattern>/DogetExample</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>InseartData</servlet-name>
		<servlet-class>com.briglabz.httpservlet.InseartData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InseartData</servlet-name>
		<url-pattern>/InseartData</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>EmpUpdateDelete</servlet-name>
		<servlet-class>com.briglabz.httpservlet.EmpUpdateDelete</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmpUpdateDelete</servlet-name>
		<url-pattern>/EmpUpdateDelete</url-pattern>
	</servlet-mapping>
</web-app>